{
  "name": "@bigzeta/datapages-client",
  "version": "0.0.0",
  "description": "Client side javascript development bundler for datapages. Basic feature at the moment is react with fabricjs intergration",
  "main": "index.js",
  "author": "jo",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "NODE_PATH=src jest",
    "hot": "cross-env NODE_ENV=development PROTOCOL=http webpack-dev-server --hot --env.stage=hot --mode=development",
    "hots": "cross-env NODE_ENV=development PROTOCOL=https webpack-dev-server --hot --env.https=true --env.stage=hot --mode=development",
    "production": "webpack -p --config=webpack.config.babel.js --env=production --mode=production",
    "development": "webpack --config=webpack.config.babel.js --env=production",
    "compile:dev": "cross-env NODE_ENV=development webpack --env.stage=dev --mode=development --profile --progress --colors",
    "compile:prod": "cross-env NODE_ENV=production webpack -p --env.stage=prod --profile --colors",
    "build:watch": "yarn run hot",
    "lint": "eslint *.js src/",
    "lint:fix": "npm run lint -- --fix",
    "test:watch": "npm run test -- --watch",
    "coverage": "NODE_PATH=src jest --coverage",
    "clean": "rimraf dist.dev dist.prod"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@vimeo/player": "2.6.3",
    "axios": "0.18.0",
    "d3": "^5.7.0",
    "formik": "^1.3.1",
    "lodash": "4.17.11",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-facebook-login": "^4.1.1",
    "react-redux": "5.0.7",
    "reactstrap": "^6.5.0",
    "redux": "4.0.0",
    "redux-form": "^7.4.2",
    "redux-saga": "0.16.1",
    "yup": "^0.26.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "autoprefixer": "9.1.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "5.6.1",
    "eslint-config-react-app": "^3.0.3",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.3",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "^2.0.0",
    "font-awesome": "4.7.x",
    "fs-extra": "7.0.0",
    "html-webpack-plugin": "3.2.0",
    "jest": "^23.6.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "^3.0.0",
    "precss": "^3.1.2",
    "promise": "8.0.2",
    "raf": "3.4.0",
    "react-dev-utils": "^6.0.4",
    "react-hot-loader": "^4.3.5",
    "react-test-renderer": "^16.5.2",
    "regenerator-runtime": "^0.12.1",
    "resolve": "1.8.1",
    "sass-loader": "^7.0.1",
    "sinon": "^6.3.5",
    "style-loader": "0.23.0",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "1.1.1",
    "webpack": "^4.8.3",
    "webpack-bundle-tracker": "^0.3.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "unmockedModulePathPatterns": [
      "react",
      "enzyme",
      "jest-enzyme"
    ],
    "testURL": "http://localhost/"
  }
}
