{
  "name": "@bigzeta/datapages-client",
  "version": "0.0.0",
  "description": "Client side javascript development bundler for datapages. Basic feature at the moment is react with fabricjs intergration",
  "main": "index.js",
  "author": "jo",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "NODE_PATH=src jest",
    "hot": "cross-env NODE_ENV=development webpack-dev-server --hot --env.stage=hot --mode=development",
    "hots": "cross-env NODE_ENV=development PROTOCOL=https webpack-dev-server --hot --env.https=true --env.stage=hot --mode=development",
    "production": "webpack -p --config=webpack.config.babel.js --env=production --mode=production",
    "development": "webpack --config=webpack.config.babel.js --env=production",
    "compile:dev": "cross-env NODE_ENV=development webpack --env.stage=dev --mode=development --profile --progress --colors",
    "compile:prod": "cross-env NODE_ENV=production webpack -p --env.stage=prod --profile --colors",
    "build:watch": "yarn run hot",
    "lint": "eslint *.js src/",
    "lint:fix": "npm run lint -- --fix",
    "test:watch": "npm run test -- --watch",
    "coverage": "NODE_PATH=src jest --coverage",
    "clean": "rimraf dist.dev dist.prod"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@babel/runtime": "^7.2.0",
    "@vimeo/player": "2.6.5",
    "axios": "0.18.0",
    "d3": "^5.7.0",
    "formik": "^1.4.1",
    "lodash": "4.17.11",
    "ramda": "^0.26.1",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-facebook-login": "^4.1.1",
    "react-redux": "6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.2.0",
    "reactstrap": "^6.5.0",
    "redux": "4.0.1",
    "redux-actions": "^2.6.4",
    "redux-form": "^8.1.0",
    "redux-saga": "0.16.2",
    "yup": "^0.26.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@storybook/addon-actions": "^4.1.4",
    "@storybook/addon-links": "^4.1.4",
    "@storybook/addons": "^4.1.4",
    "@storybook/react": "^4.1.4",
    "alias-quokka-plugin": "^0.1.0",
    "autoprefixer": "^9.4.3",
    "axios-mock-adapter": "^1.16.0",
    "babel-alias-quokka-plugin": "^1.0.9",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "dotenv": "6.2.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "5.11.1",
    "eslint-config-react-app": "^3.0.6",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-flowtype": "3.2.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "^7.12.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "^3.0.1",
    "font-awesome": "4.7.x",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-dom": "^3.0.0",
    "jest-enzyme": "^7.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-noop": "^1.0.0",
    "node-sass": "^4.9.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "promise": "8.0.2",
    "prop-types": "^15.6.2",
    "raf": "3.4.1",
    "react-dev-utils": "^7.0.0",
    "react-hot-loader": "^4.6.3",
    "react-test-renderer": "^16.7.0",
    "regenerator-runtime": "^0.13.1",
    "resolve": "1.9.0",
    "sass-loader": "^7.0.1",
    "sinon": "^7.2.2",
    "style-loader": "0.23.1",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "1.1.2",
    "wallaby-webpack": "^3.9.13",
    "webpack": "^4.28.2",
    "webpack-bundle-tracker": "^0.3.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.1.2"
  },
  "jest": {
    "testURL": "http://localhost/",
    "moduleNameMapper": {
      "~/(.*)": "<rootDir>/src/$1",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "transform": {
      "\\.(scss|css)$": "<rootDir>/fileTransformer.js"
    }
  },
  "quokka": {
    "babel": true,
    "plugins": [
      "babel-alias-quokka-plugin"
    ],
    "alias": {
      "~": "./src"
    },
    "pro": true
  }
}
