{
    "name": "@bigzeta/datapages-client",
    "version": "0.0.0",
    "description": "Client side javascript development bundler for datapages. Basic feature at the moment is react with fabricjs intergration",
    "main": "index.js",
    "author": "jo",
    "license": "UNLICENSED",
    "scripts": {
        "hot": "cross-env NODE_ENV=development webpack-dev-server --hot --env.stage=hot --mode=development",
        "production": "webpack -p --config=webpack.config.babel.js --env=production --mode=production",
        "development": "webpack --config=webpack.config.babel.js --env=production",
        "compile:dev": "cross-env NODE_ENV=development webpack --env.stage=dev --mode=development --profile --progress --colors",
        "compile:prod": "cross-env NODE_ENV=production webpack -p --env.stage=prod --profile --colors",
        "lint": "eslint *.js src/",
        "lint:fix": "npm run lint -- --fix",
        "test": "NODE_PATH=src jest",
        "test:watch": "npm run test -- --watch",
        "coverage": "NODE_PATH=src jest --coverage",
        "clean": "rimraf dist.dev dist.prod"
    },
    "devDependencies": {
        "babel": "^6.23.0",
        "babel-cli": "^6.26.0",
        "babel-eslint": "^8.2.3",
        "babel-loader": "^7.1.4",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-regenerator": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "cors": "^2.8.4",
        "cross-env": "^5.2.0",
        "css-loader": "^0.28.11",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "eslint": "^4.19.1",
        "eslint-loader": "^2.0.0",
        "eslint-plugin-babel": "^5.1.0",
        "eslint-plugin-react": "^7.8.2",
        "extract-text-webpack-plugin": "next",
        "file-loader": "^1.1.11",
        "font-awesome": "4.7.x",
        "jest": "^22.4.4",
        "jest-enzyme": "^6.0.0",
        "mini-css-extract-plugin": "^0.4.1",
        "node-sass": "^4.9.2",
        "postcss-loader": "^2.1.5",
        "precss": "^3.1.2",
        "react-hot-loader": "^4.2.0",
        "rimraf": "^2.6.2",
        "sass-loader": "^7.0.1",
        "sinon": "^5.0.7",
        "style-loader": "^0.21.0",
        "url-loader": "^1.0.1",
        "webpack": "^4.8.3",
        "webpack-bundle-tracker": "^0.3.0",
        "webpack-cli": "^2.1.3",
        "webpack-dev-server": "^3.1.4",
        "webpack-merge": "^4.1.2"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "d3": "^5.4.0",
        "d3-selection-multi": "^1.0.1",
        "html2canvas": "^1.0.0-alpha.12",
        "jspdf": "^1.4.1",
        "jump.js": "^1.0.2",
        "lodash": "^4.17.10",
        "react": "^16.3.2",
        "react-dom": "^16.3.2",
        "react-redux": "^5.0.7",
        "react-treebeard": "^2.1.0",
        "reactstrap": "^6.3.0",
        "redux": "^4.0.0",
        "redux-module-builder": "^0.3.2",
        "redux-saga": "^0.16.0"
    },
    "jest": {
        "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
        "unmockedModulePathPatterns": [
            "react",
            "enzyme",
            "jest-enzyme"
        ]
    }
}
