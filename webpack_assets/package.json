{
  "name": "webpack_assets",
  "version": "1.0.0",
  "description": "Build assets for GardenTronic",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && cross-env NODE_ENV=development webpack --env.stage=dev --mode=development",
    "build:prod": "cross-env NODE_ENV=development webpack --env.stage=prod --mode=production",
    "build:ci": "rimraf dist.prod && cross-env NODE_ENV=circleci webpack --env.stage=circleci --mode=production",
    "clean": "rimraf ../django_root/static/css_webpack ../django_root/static/assets",
    "watch": "cross-env NODE_ENV=development webpack --mode=development --env.stage=dev --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "bootstrap": "^4.3.1",
    "cors": "^2.8.5",
    "cross-env": "^5.2.1",
    "css-loader": "^3.2.0",
    "dotenv": "^8.1.0",
    "extract-loader": "^3.1.0",
    "file-loader": "^4.2.0",
    "font-awesome": "^4.7.0",
    "glob": "^7.1.4",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "raf": "^3.4.1",
    "resolve": "^1.12.0",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.3",
    "webpack-bundle-tracker": "^0.4.2-beta",
    "webpack-cli": "^3.3.7",
    "webpack-glob-entry": "^2.1.1",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "core-js": "3.2.1",
    "draftail": "^1.3.0"
  }
}
